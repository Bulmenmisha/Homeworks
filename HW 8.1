function calculateProduct(numbers) {
    let product = 1; 
    for (let i = 0; i < numbers.length; i++) {
      product *= numbers[i]; 
    }
    return product;
  }
  const array = [2, 3, 4, 5];
  const result = calculateProduct(array);
  console.log(`Произведение всех элементов массива: ${result}`);



  
  
  
  
